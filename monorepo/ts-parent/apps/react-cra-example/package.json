{
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "eslint-config-react-app": "^7.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "3.1.0"
  },
  "devDependencies": {
    "@tsconfig/node16-strictest": "1.0.4",
    "@bazel/ibazel": "0.16.2",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "typescript": "4.9.3"
  },

  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "pnpm": {
    "packageExtensions": {
      "@typescript-eslint/eslint-plugin": {
        "peerDependencies": {
          "eslint": "*"
        }
      },
      "postcss-loader": {
        "peerDependencies": {
          "postcss-flexbugs-fixes": "*",
          "postcss-preset-env": "*",
          "postcss-normalize": "*"
        }
      }
    }
  }
}

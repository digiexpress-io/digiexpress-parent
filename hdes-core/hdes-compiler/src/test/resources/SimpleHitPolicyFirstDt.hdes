decision-table SimpleHitPolicyFirstDt {
  accepts { name STRING, lastName STRING }
  returns { value INTEGER }

  matches FIRST {
    when { _ = 'sam', ? }       then { 20 }
    when { _ = 'bob',  _ = 'woman'}  then { 4570 } 
    when { !(_ IN('bob','same', 'professor')), !(_ IN('woman', 'man'))} then { 4570 }
  }
  
}
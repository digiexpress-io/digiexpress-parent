        decisiontable DtWithFormula
        (
          a: INTEGER, b: INTEGER, c: DECIMAL,
          total: DECIMAL = a + b + c):
        {
          totalOut: DECIMAL = total,
          score: STRING } {
        
          findFirst(){ 
            when(undefined, undefined, undefined, _ > 100)   add('high-risk') 
            when(undefined, undefined, undefined, undefined) add('low-risk') 
          }
        }
define flow: SimpleFlow description: 'descriptive'
headers: {
  arg1 INTEGER optional IN,
  arg2 INTEGER optional IN,
  arg3 ARRAY INTEGER optional IN,
  arg4 ARRAY OBJECT optional IN: {
    arg5 INTEGER optional IN,
    arg6 INTEGER optional IN
  }
}
tasks: {
  firstTask: {
    then: switch
    decisionTask: BestDt mapping OBJECT: {
      name: arg1,
      lastName: arg2
    }
  },
  switch: {
    when: arg1 > 10 then: secondTask,
    when: ? then: end mapping OBJECT: {
      name: arg1,
      lastName: arg2
    }
  },
  secondTask: {
    then: end mapping OBJECT: { }
    decisionTask: Best2Dt mapping OBJECT: {
      name: arg1,
      lastName: arg2
    }
  }
}